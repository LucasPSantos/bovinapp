<div class="container mt-3">
    <div class="row">
        <div class="col-md-12">
            <h1 class="mt-3 mb-4 text-center">Lista de rebanho</h1>
            <table class="table table-striped" style="font-size: 14px">
                <thead>
                <tr>
                    <th class="align-middle text-center">NOME DE REFERENCIA PARA O REBANHO</th>
                    <th class="align-middle text-center">DATA DE CADASTRO</th>
                    <th class="align-middle text-center">DESCRIÇÃO</th>
                    <th class="align-middle text-center">Editar | Remover </th>
                </tr>
                </thead>
                <tbody>
                <% rebanhos.forEach((rebanho) => { %>
                    <tr>
                        <td class="align-middle text-center"><%- rebanho.nome %></td>
                        <td class="align-middle text-center"><%- rebanho.dataentrada %></td>
                        <td class="align-middle text-center"><%- rebanho.desc %></td>
                        <td class="align-middle text-center">
                            <a href="/rebanho/edit/<%-rebanho._id.toString()%>"><button class="btn btn-outline-primary"><i class="fas fa-pencil-alt"></i></button></a>
                          </td>
                          <td class="align-middle text-center">
                            <a href="/rebanho/delete/<%-rebanho._id.toString()%>"><button class="btn btn-outline-primary"><i class="fas fa-trash"></i></button></a>
                        </td>
                    </tr>
                <% }) %>
                </tbody>
            </table>
        </div>
    </div>
</div>
