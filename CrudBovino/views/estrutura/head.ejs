    if((ev!=6) && (ev!=8) &&(ev!=13))     
      if  (! RegExp(/[0-9]/gi).test(String.fromCharCode(ev))) 
        return false;           
      if(element.value + String.fromCharCode(ev) > 9999) 
    return false;     
} 
